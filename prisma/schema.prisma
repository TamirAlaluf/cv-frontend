datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                Int      @id @default(autoincrement())
  name              String  
  email             String   @unique
  username          String?  
  number_usage_left Int      @default(5)
  createdAt         DateTime @default(now())
  clerk_id          String?  
}
